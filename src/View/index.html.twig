{% extends "base.html.twig" %}


{% block products %}
    <div class="container">
        <div class="row">
        {% for product in products %}
            <div class="col-md-4">
                <div class="card" style="width: 18rem;">
                    <img class="card-img-top" src="https://via.placeholder.com/150" alt="Card image cap">
                    <h5 class="card-title">{{ product }}</h5>
                </div>
            </div>
        {% endfor %}
        </div>
    </div>
{% endblock %}
